{
  "metadata": [
    {
      "src": [
        { "src": "../", "files": [ "src/LpSolveDotNet/LpSolveDotNet.csproj" ] }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml",
      "shouldSkipMarkup": false,
      "properties": {
        "TargetFramework": "net471"
      }
    }
  ],
  "build": {
    "content": [
      { "files": [ "api/**.yml", "api/index.md" ] },
      { "files": [ "articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md" ] }
    ],
    "resource": [
      { "files": [ "images/**" ] }
    ],
    "overwrite": [
      { "files": [ "apidoc/**.md" ], "exclude": [ "obj/**", "_site/**" ] }
    ],
    "globalMetadata": {
      "_appTitle": "Test website",
      "_enableSearch": false,
      "_appFooter": "LpSolveDotNet documentation - generated by DocFX",
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc": false
    },
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "common"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}